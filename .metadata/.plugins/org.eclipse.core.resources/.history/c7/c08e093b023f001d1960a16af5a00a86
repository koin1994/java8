package sec3;

public class MotorCycle {
	FrontTire = new Tire(6, "앞쪽");
	BackTire = new Tire(7, "뒤쪽");
	
	public MotorCycle () {}

	public int run () {
		System.out.println("오토바이가 달립니다.");
		if(FrontTire.roll()==false) { stop(); return 1;}
		if(BackTire.roll()==false) { stop(); return 2;}
		return = 0
		}
	public void stop(){
		System.out.println("오토바이가 멈춥니다. ");
	}
}

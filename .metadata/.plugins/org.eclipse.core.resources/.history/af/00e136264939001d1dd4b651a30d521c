package sec2;

public class StudentEx {
	public static void main(String[] args) {
		Student student1 = new Student("김기태",90,80,90);
		System.out.print("이름 :"+student1.name);
		System.out.print("총점 :"+student1.getTot());
		System.out.print("평균 :"+student1.getAvg());
		System.out.print("학점 :"+student1.getHak());
	}
}
